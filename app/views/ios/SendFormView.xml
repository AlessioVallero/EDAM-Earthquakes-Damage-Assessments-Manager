<Alloy>
    <NavigationWindow id="navigationWindowSendFormView">
        <Window id="viewSendFormViewWindow">
            <LeftNavButton>
                <Button id="btn_ios_back" onClick="OnBtnBack_Click" />
            </LeftNavButton>
            <RightNavButton>
                <Button id="btn_ios_next" onClick="OnBtnNext_Click" />
            </RightNavButton>

            <ActivityIndicator id="activity_indicator" />

            <ScrollView id="scrollViewSendForm">
                <View id="viewAppCheckBoxSendFormViewPreview">
                    <Widget id="widgetAppCheckBoxSendFormViewPreview" src="com.diseg.AppCheckBox" />
                </View>
                <View id="viewAppCheckBoxSendFormViewMediaContents">
                    <Widget id="widgetAppCheckBoxSendFormViewMediaContents" src="com.diseg.AppCheckBox" />
                </View>

                <ImageView id="imageViewSendEmailLogo" />
            </ScrollView>
        </Window>
    </NavigationWindow>
</Alloy>